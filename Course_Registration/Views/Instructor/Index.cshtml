@model IEnumerable<Instructor>

<h2>Instructor View</h2>
<p>
    <a asp-action="Create">Create New Instructor</a>
</p>
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col" style="white-space: nowrap;">Id</th>
            <th scope="col" style="white-space: nowrap;">Course Id</th>
            <th scope="col" style="white-space: nowrap;">First Name</th>
            <th scope="col" style="white-space: nowrap;">Last Name</th>
            <th scope="col" style="white-space: nowrap;">Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var v in Model)
        {
            <tr>
                <td>@v.Id</td>
                <td>@v.CourseId</td>
                <td>@v.FirstName</td>
                <td>@v.LastName</td>
                <td>@v.Email</td>
            </tr>
        }
    <tbody>
</table>